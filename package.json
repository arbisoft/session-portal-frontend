{
  "name": "session-portal",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:e2e": "cp -n example.env.local .env.local && next build",
    "start": "next start",
    "typecheck": "./node_modules/.bin/tsc --project ./tsconfig.json",
    "lint": "next lint && npm run typecheck",
    "prepare": "is-ci || husky || true",
    "release": "release-it",
    "generate:resource": "hygen generate resource",
    "postgenerate:resource": "npm run lint -- --fix",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest --verbose",
    "test:cov": "jest --coverage --verbose"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.2.0",
    "@mui/icons-material": "6.1.6",
    "@mui/material": "6.1.6",
    "@mui/x-date-pickers": "7.22.2",
    "@react-oauth/google": "^0.12.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@vidstack/react": "^1.12.12",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "next": "^14.2.26",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.45.4",
    "react-redux": "^9.2.0",
    "react-virtuoso": "^4.12.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@faker-js/faker": "^9.7.0",
    "@playwright/test": "^1.43.1",
    "@release-it/conventional-changelog": "^10.0.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.5.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-storybook": "^0.11.0",
    "husky": "^9.0.10",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.2.5",
    "release-it": "^18.1.2",
    "storybook": "^8.6.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "whatwg-fetch": "^3.6.20"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "perf",
              "section": "Performance Improvements"
            },
            {
              "type": "revert",
              "section": "Reverts"
            },
            {
              "type": "docs",
              "section": "Documentation"
            },
            {
              "type": "refactor",
              "section": "Code Refactoring"
            },
            {
              "type": "test",
              "section": "Tests"
            },
            {
              "type": "ci",
              "section": "Continuous Integration"
            }
          ]
        }
      }
    }
  }
}
